  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "apollo-server-express": "^2.17.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.5",
    "pg": "^8.3.3",
    "uuid": "^8.3.0"
  }
